{
    "extends": [
        "eslint:recommended"
    ],
    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "globals": {
    },
    "rules": {
        "semi": 2,
        "no-console": 0,
        "no-unused-vars": 1,
        "react/prop-types": 0
    }
}
